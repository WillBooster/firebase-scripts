{
  "name": "@firebase-scripts/shared",
  "version": "0.0.0-semantically-released",
  "private": true,
  "repository": "github:WillBooster/firebase-scripts",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__,scripts}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "firebase-admin": "11.3.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.7.1",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "@willbooster/eslint-config-ts": "10.0.8",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.4.2",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-class-members": "1.15.2",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "44.0.2",
    "lint-staged": "13.0.3",
    "micromatch": "4.0.5",
    "prettier": "2.7.1",
    "sort-package-json": "2.1.0",
    "typescript": "4.9.3"
  },
  "packageManager": "yarn@4.0.0-rc.30"
}
